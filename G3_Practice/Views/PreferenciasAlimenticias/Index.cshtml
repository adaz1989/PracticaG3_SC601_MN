@model IEnumerable<G3_Practice.Models.PreferenciaAlimenticia>

@{
    ViewBag.Title = "Preferencias Alimenticias";
}

<p>
    @Html.ActionLink("Crear nueva Preferencia Alimenticia", "Crear")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.Id } ) |
                @Html.ActionLink("Detalles", "Detalles", new { id = item.Id }) |
                @Html.ActionLink("Eliminar", "Eliminar", new { id = item.Id })
            </td>
        </tr>
    }

</table>
